{
  "name": "dss-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "dss-side-menu:build": "ng build dss-side-menu",
    "dss-side-menu:lint": "ng lint dss-side-menu",
    "dss-side-menu:test": "ng test dss-side-menu --no-watch --no-progress --browsers=ChromiumHeadless",
    "dss-side-menu:publish": "npm run dss-side-menu:test && npm run dss-side-menu:build && npm publish dist/dss-side-menu/"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.5",
    "@angular/compiler": "^21.1.5",
    "@angular/core": "^21.1.5",
    "@angular/forms": "^21.1.5",
    "@angular/platform-browser": "^21.1.5",
    "@angular/router": "^21.1.5",
    "@tailwindcss/postcss": "^4.2.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.2.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.5",
    "@types/jasmine": "~5.1.0",
    "@vitest/browser-playwright": "^4.0.18",
    "angular-eslint": "21.2.0",
    "eslint": "^9.39.2",
    "jasmine-core": "~5.9.0",
    "jsdom": "^28.1.0",
    "ng-packagr": "^21.1.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.50.1",
    "vitest": "^4.0.18"
  }
}
