    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-server": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@angular/ssr": "^21.1.2",
    "@tailwindcss/postcss": "^4.1.18",
    "express": "^5.1.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^21.1.2",
    "@angular/compiler-cli": "^21.1.2",
    "@netlify/angular-runtime": "^3.0.1",
    "@types/express": "^5.0.1",
